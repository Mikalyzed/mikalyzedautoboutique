# AWS credentials for DynamoDB access
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1

# DynamoDB table name
DYNAMODB_TABLE_NAME=mikalyzed-vehicles

# API key for the inventory sync endpoint (generate a strong random string)
# Used by AWS Lambda as x-api-key header or Authorization: Bearer token
SYNC_API_KEY=

# Clerk authentication (get keys at https://dashboard.clerk.com)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/admin

# Admin user IDs (comma-separated Clerk user IDs for admin access)
ADMIN_USER_IDS=

# DynamoDB table names for admin features
DYNAMODB_LEADS_TABLE_NAME=mikalyzed-leads
DYNAMODB_BLOG_TABLE_NAME=mikalyzed-blog

# S3 bucket for sell car image uploads
S3_UPLOAD_BUCKET=mikalyzed-uploads
